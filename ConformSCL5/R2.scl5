R2 DEFINITIONS ::= BEGIN

   EXPORTS PDU;

   PDU ::= SEQUENCE {
       a REAL    (SIZE 8 BYTES) BIGENDIAN,
       b INTEGER (SIZE 8 BYTES) BIGENDIAN,
       c REAL    (SIZE 4 BYTES) BIGENDIAN,
       d INTEGER (SIZE 4 BYTES) BIGENDIAN,
       e INTEGER (SIZE 2 BYTES) BIGENDIAN,
       f INTEGER (SIZE 1 BYTES),
   } (ENCODED BY CUSTOM)

END
